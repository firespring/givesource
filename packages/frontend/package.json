	"scripts": {
		"cache": "node bin/clear-cache",
		"clean": "rimraf build",
		"build": "npm run clean && npm run fetch && npm run webpack",
		"deploy": "npm run build && node bin/deploy.js",
		"dev": "npm run clean && npm run fetch && npm run webpack:watch",
		"fetch": "node bin/fetch-settings",
		"release": "npm run webpack && node bin/release.js",
		"release:force": "npm run webpack && node bin/release.js --force",
		"webpack": "webpack --config config/webpack.config.admin.js --config config/webpack.config.public.js --hide-modules",
		"webpack:watch": "webpack --config config/webpack.config.admin.js --config config/webpack.config.public.js --hide-modules --watch"
	},
