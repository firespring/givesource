{
  "name": "givesource-lambda",
  "description": "AWS Lambda functions for GivesourceÂ®",
  "scripts": {
    "build": "npm run clean && npm run webpack",
    "clean": "rimraf build/*",
    "delete-test-payments": "node bin/delete-test-payments",
    "delete-payments-by-transaction-ids": "node bin/delete-payments-by-transaction-ids",
    "deploy": "npm run build && node bin/deploy",
    "release": "npm run build && node bin/release",
    "release:force": "npm run release -- --force",
    "seed": "node bin/seed",
    "setting": "node bin/update-setting",
    "test": "NODE_ENV=development mocha test/ --recursive",
    "test:coverage": "nyc --reporter=lcov --reporter=text --include src/ --all npm run test",
    "test:dev": "npm run test -- --watch",
    "webpack": "webpack --config config/webpack.config.js"
  },
